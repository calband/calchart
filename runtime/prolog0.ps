/smoveto { transform round exch round exch itransform moveto } def
/slineto { transform round exch round exch itransform lineto } def
/dotplain {
   /y exch def
   /x exch def
   newpath
   x y w 0 360 arc
   closepath
   stroke
} bind def
/dotsolid {
   /y exch def
   /x exch def
   newpath
   x y w 0 360 arc
   closepath
   fill
} bind def
/dotbs {
   /y exch def
   /x exch def
   newpath
   x y w 0 360 arc
   closepath
   stroke
   x plinew sub y plinew add smoveto
   x plinew add y plinew sub slineto
   stroke
} bind def
/dotsl {
   /y exch def
   /x exch def
   newpath
   x y w 0 360 arc
   closepath
   stroke
   x plinew add y plinew add smoveto
   x plinew sub y plinew sub slineto
   stroke
} bind def
/dotx {
   /y exch def
   /x exch def
   newpath
   x y w 0 360 arc
   closepath
   stroke
   x plinew sub y plinew add smoveto
   x plinew add y plinew sub slineto
   stroke
   x plinew add y plinew add smoveto
   x plinew sub y plinew sub slineto
   stroke
} bind def
/dotsolbs {
   /y exch def
   /x exch def
   newpath
   x y w 0 360 arc
   closepath
   fill
   x slinew sub y slinew add smoveto
   x slinew add y slinew sub slineto
